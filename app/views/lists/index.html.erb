<h1>My Lists</h1>


<% @lists.each do | list | %>
  <div class="card" style="width: 18rem;">
    <img src="/app/assets/images/comedy-6054626_1280.jpg" class="card-img-top" alt="<%list.name%>">
    <div class="card-body">
      <h5 class="card-title"><%= list.name %></h5>
      <%= link_to "Show this list", list_path(list) %>
    </div>
  </div>
<% end %>


<%= link_to "Create a new list", new_list_path %>

